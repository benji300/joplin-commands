{
  "name": "joplin-plugin-benji300-commands",
  "version": "1.0.0",
  "description": "Collection of commands to improve and speed up your daily work with Joplin.",
  "author": "Benji300",
  "homepage": "https://github.com/benji300/joplin-commands",
  "bugs": {
    "url": "https://github.com/benji300/joplin-commands/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benji300/joplin-commands.git"
  },
  "scripts": {
    "dist": "webpack --joplin-plugin-config buildMain && webpack --joplin-plugin-config buildExtraScripts && webpack --joplin-plugin-config createArchive",
    "prepare": "npm run dist",
    "update": "npm install -g generator-joplin && yo joplin --update"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "browser": {
    "child_process": false
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "on-build-webpack": "^0.1.0",
    "tar": "^6.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.1"
  }
}
